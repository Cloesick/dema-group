# Authentication
AUTH_SECRET=your-auth-secret-here
NEXTAUTH_URL=https://dema-group-portal.vercel.app
JWT_SECRET=your-jwt-secret-here
JWT_REFRESH_SECRET=your-jwt-refresh-secret-here

# Database
DATABASE_URL=your-database-url-here
REDIS_URL=redis://localhost:6379

# API Keys
API_KEY=your-api-key-here
VERCEL_API_TOKEN=your-vercel-api-token-here
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# Vercel Specific
VERCEL_ENV=production
VERCEL_URL=dema-group-portal.vercel.app
VERCEL_REGION=iad1
VERCEL_GIT_COMMIT_SHA=
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_API_URL=https://api.dema-group-portal.vercel.app

# Feature Flags
ENABLE_API_LOGGING=true
ENABLE_PERFORMANCE_MONITORING=true
LOG_LEVEL=info

# Webhooks
WEBHOOK_SECRET_DEMA=your-dema-webhook-secret-here
WEBHOOK_SECRET_FLUXER=your-fluxer-webhook-secret-here
WEBHOOK_SECRET_BELTZ247=your-beltz247-webhook-secret-here
WEBHOOK_SECRET_DEVISSCHERE=your-devisschere-webhook-secret-here
WEBHOOK_SECRET_ACCU=your-accu-webhook-secret-here

# Inventory
INVENTORY_API_KEYS=key1,key2,key3

# Build
ANALYZE=false
