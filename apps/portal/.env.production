# Feature Flags
ENABLE_API_LOGGING=true
ENABLE_PERFORMANCE_MONITORING=true
LOG_LEVEL=info

# Build
ANALYZE=false
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=production

# App Version
NEXT_PUBLIC_APP_VERSION=1.0.0

# API URLs (these will be overridden by Vercel)
NEXT_PUBLIC_API_URL=https://api.dema-group-portal.vercel.app
NEXTAUTH_URL=https://dema-group-portal.vercel.app
