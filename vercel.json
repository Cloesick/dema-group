{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "cd apps/portal && pnpm install --no-frozen-lockfile && pnpm build",
  "outputDirectory": "apps/portal/.next",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "devCommand": "cd apps/portal && pnpm dev",
  "regions": ["arn1"],
  "public": false,
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/portal/",
  "git": {
    "deploymentEnabled": {
      "main": true,
      "master": true
    }
  }
}
